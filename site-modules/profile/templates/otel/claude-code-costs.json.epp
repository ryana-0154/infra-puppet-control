{
  "dashboard": {
    "id": null,
    "title": "Claude Code Cost Analysis",
    "tags": ["claude", "costs", "billing"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "1m",
    "schemaVersion": 16,
    "version": 0,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Daily Cost Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(claude_cost_usd_total[24h])",
            "refId": "A",
            "legendFormat": "Daily Cost",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "yAxes": [
          {
            "label": "USD"
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "min": false,
          "max": true,
          "current": true,
          "total": true,
          "avg": true
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Cost Breakdown by Model",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (model) (increase(claude_cost_usd_total[24h]))",
            "refId": "A",
            "legendFormat": "{{model}}",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Token Cost Efficiency",
        "type": "graph",
        "targets": [
          {
            "expr": "(increase(claude_cost_usd_total[1h]) / increase(claude_tokens_total[1h])) * 1000000",
            "refId": "A",
            "legendFormat": "Cost per Million Tokens",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "yAxes": [
          {
            "label": "USD per 1M tokens"
          }
        ],
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": true,
          "total": false,
          "avg": false
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "Cache Savings",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_cache_tokens_saved_total[1h])",
            "refId": "A",
            "legendFormat": "Tokens Saved/Hour",
            "datasource": "<%= $datasource_name %>"
          },
          {
            "expr": "rate(claude_cache_cost_saved_usd_total[1h])",
            "refId": "B",
            "legendFormat": "Cost Saved/Hour (USD)",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens",
            "side": "left"
          },
          {
            "label": "USD",
            "side": "right"
          }
        ],
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": true,
          "total": false,
          "avg": false
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Hourly Usage Pattern",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (hour) (rate(claude_sessions_total[1h]))",
            "refId": "A",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ]
  }
}
