{
  "dashboard": {
    "id": null,
    "title": "Claude Code Overview",
    "tags": ["claude", "opentelemetry", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 0,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Total Sessions",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "increase(claude_sessions_total[1h])",
            "refId": "A",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "valueFontSize": "80%",
        "valueName": "current",
        "colorBackground": false,
        "colorValue": true,
        "thresholds": "1,5",
        "colors": ["#d44a3a", "#e24d42", "#50a64f"],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Total Tokens Used",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "sum(increase(claude_tokens_total[1h]))",
            "refId": "A",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "valueFontSize": "80%",
        "valueName": "current",
        "format": "short",
        "colorBackground": false,
        "colorValue": true,
        "thresholds": "10000,50000",
        "colors": ["#50a64f", "#eab839", "#d44a3a"],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Total Cost (USD)",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "sum(increase(claude_cost_usd_total[1h]))",
            "refId": "A",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "valueFontSize": "80%",
        "valueName": "current",
        "format": "currencyUSD",
        "colorBackground": false,
        "colorValue": true,
        "thresholds": "1,10",
        "colors": ["#50a64f", "#eab839", "#d44a3a"],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "singlestat",
        "span": 3,
        "targets": [
          {
            "expr": "rate(claude_cache_hits_total[5m]) / (rate(claude_cache_hits_total[5m]) + rate(claude_cache_misses_total[5m])) * 100",
            "refId": "A",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "valueFontSize": "80%",
        "valueName": "current",
        "format": "percent",
        "colorBackground": false,
        "colorValue": true,
        "thresholds": "50,80",
        "colors": ["#d44a3a", "#eab839", "#50a64f"],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Token Usage by Model",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_tokens_total[5m]) by (model)",
            "refId": "A",
            "legendFormat": "{{model}}",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens/sec"
          }
        ],
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Cost by Model (Hourly)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(claude_cost_usd_total[1h]) by (model)",
            "refId": "A",
            "legendFormat": "{{model}}",
            "datasource": "<%= $datasource_name %>"
          }
        ],
        "yAxes": [
          {
            "label": "USD/hour"
          }
        ],
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ]
  }
}
