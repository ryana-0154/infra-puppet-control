# Grafana dashboard provisioning configuration
# Managed by Puppet - do not edit manually

apiVersion: 1

providers:
  - name: 'Loki Logs'
    orgId: 1
    folder: 'Logs'
    type: file
    disableDeletion: false
    updateIntervalSeconds: 10
    allowUiUpdates: true
    options:
      path: /etc/grafana/provisioning/dashboards
